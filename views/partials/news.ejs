  -webkit-text-fill-color: transparent;
}
.truncate3.blog-description{
  position: relative;
}
.truncate3.blog-description:after{
  content: "";
  position: absolute;
  top: 3rem;
  bottom: 0;
  left: 0;
  right: 0;
  box-shadow: inset 0 -2.5rem 1rem -1.5rem white;
}

@media (max-width: 767px) {
  .blog-card {
    width: 100%;
    margin-left: 0.25rem !important;
    margin-right: 0.25rem !important;
  }
  .blog-card.noborder {
    margin-left: auto !important;
    margin-right: auto !important;
  }
  .searchdp.blogsdp{
    height: 50px !important;
    width: 50px !important;
  }
  .card-pad{
    padding: 1rem 0;
  }
  .truncate5.news-description{
    -webkit-line-clamp: 8 !important;
  }
}
</style>


