}
.blogcount{
  min-width: 0.5rem;
}
.bookmarked{
  -webkit-text-stroke: 0 #262626;
  color: #c73b3c;
  text-shadow: 0.5px 0.5px 1px rgb(0 0 0 / 15%);
  transform: scaleY(2.5) translateY(0.375rem);
  background: -webkit-linear-gradient(top, #c73b3c 0%,#b8201f 100%);
