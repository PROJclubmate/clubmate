<% include ../partials/header %>

<div class="mt-4 pt-3">
  <h3 class="py-1"> Audio Rooms </h3>

    <% for (const club_rooms of audioroomsData) { %>
      <div class="card py-3">

        <div class="mt-2">
          <div class="club_name"> <%= club_rooms.club_name %> </div>
          <div id="common-room-div" class="room-container-div">

              <% for (const audio_room of club_rooms.audio_rooms) { %>
                <div class="room-container whitesmokeback px-3 col-sm-3">
                  <a href="/audio/join/<%= audio_room.roomId %>"> <%= audio_room.roomName %> </a>
                </div>
              <% } %>

            </a>
          </div>
          
          
        </div>

      </div>
    <% } %>
</div>

<% include ../partials/footer %>