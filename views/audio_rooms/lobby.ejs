<% include ../partials/header %>

<div class="mt-4 pt-3">
  <h3 class="py-1"> Lobby </h3>

    <% for (const club of audioroomsData) { %>
      <div class="py-3">

        <div class="mt-2 mx-4">
          <div class="lobby_club_name">
            <!-- TODO : Show club DP here CSS -->
            <span>
              <img src="<%= club.avatar_100 %>">
            </span>
            <a href="/clubs/<%= club._id %>"> <%= club.name %> </a>
          </div>
          <div id="common-room-div" class="row">

              <% for (const audio_room of club.audio_rooms) { %>
                <div class="px-1 col-sm-3 my-2">
                  <div class="room-container whiteback">
                    <a href="/audio/join/<%= audio_room.roomId %>"> <%= audio_room.roomName %> </a>
                  </div>
                </div>
              <% } %>

            </a>
          </div>
          
          
        </div>

      </div>
    <% } %>
</div>

<% include ../partials/footer %>